<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ActionHandler">
    <children>
      <Button layoutX="676.0" layoutY="350.0" mnemonicParsing="false" onAction="#searchDocuments" prefHeight="50.0" prefWidth="100.0" text="Search" textAlignment="CENTER">
         <font>
            <Font />
         </font>
      </Button>
      <TextField fx:id="searchInput" layoutX="170.0" layoutY="350.0" prefHeight="50.0" prefWidth="500.0" promptText="Type here to begin searching">
         <font>
            <Font size="19.0" />
         </font>
      </TextField>
    </children>
</AnchorPane>
