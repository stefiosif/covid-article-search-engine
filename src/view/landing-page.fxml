<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ActionHandler">
      <Button layoutX="615.0" layoutY="260.0" mnemonicParsing="false" onAction="#searchDocuments" prefHeight="50.0" prefWidth="100.0" text="Search" textAlignment="CENTER">
         <font>
            <Font name="Candara" size="22.0" />
         </font>
      </Button>
      <TextField fx:id="searchInput" layoutX="77.0" layoutY="260.0" prefHeight="50.0" prefWidth="500.0" promptText="Type here to begin searching">
         <font>
            <Font name="Candara Light" size="20.0" />
         </font>
      </TextField>
</AnchorPane>
